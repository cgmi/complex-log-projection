{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1234/",
            "webRoot": "${workspaceFolder}/complex-log-projection/src",
            "trace": true,
            "breakOnLoad": true,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "*": "${webRoot}/*"
              }
        },
        {
            "name": "Launch Firefox DE",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:1234/",
            "webRoot": "${workspaceFolder}"
        }
    ]
}